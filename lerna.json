{
  "useWorkspaces": true,
  "npmClient": "yarn",
  "command": {
    "publish": {
      "message": "chore(release): publish %s"
    }
  },
  "changelog": {
    "repo": "yueqing",
    "labels": {
      "feat": ":rocket: New Feature",
      "fix": ":bug: Bug Fix",
      "docs": ":book: Documentation",
      "chore": ":turtle: Chore",
      "break": ":boom: Breaking Change"
    },
    "cacheDir": ".changelog"
  },
  "packages": [
    "packages/*"
  ],
  "version": "1.0.1"
}
